package Array;

public class FindingMissingNumberInArray {

	public static void main(String[] args) {
		
		int [] a = {1 ,2 ,3 ,5};
		
		System.out.println("Missing Element - "+findMissingElement(a,a.length+1));

	}

	private static String findMissingElement(int[] a, int n) {
		
		int sum = (n*(n+1))/2;
		
		
		return null;
	}

}
