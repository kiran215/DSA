package Array;

public class MoveZeroesToEndOfArray {

	public static void main(String[] args) {
		int[] arr = { 0, 3, 2, 0, 4, 9, 1, 10, 6, 5, 10 };

		int newArr[] = moveZeroes(arr);
		printArray(newArr);

	}

	private static int[] moveZeroes(int[] arr) {
		//two pointer laogirthm will be used to traverse array and move zeroes to end of
		//an array
		return null;
	}

	private static void printArray(int[] newArr) {

		int n = newArr.length;

		for (int i = 0; i < n; i++) {
			System.out.print(newArr[i] + " ");
		}
		System.out.println();

	}

}
